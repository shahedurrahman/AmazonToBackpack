{
  "name": "Amazon To Backpack",
  "description": "Easily open Amazon product page links in Backpack.",
  "version": "1.4",
  "manifest_version": 2,
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_title": "Open in Backpack"
  },
  "permissions": [
    "declarativeContent"
  ]
}